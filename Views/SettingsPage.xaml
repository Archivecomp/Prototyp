<?xml version="1.0" encoding="utf-8" ?>
<Page x:Class="neuesmodell.Views.SettingsPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
      xmlns:local="using:neuesmodell"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
      xmlns:views="using:neuesmodell.Views"
      xmlns:wuc="using:WinUICommunity"
      wuc:BreadcrumbNavigator.IsHeaderVisible="True"
      mc:Ignorable="d">
    <ScrollView Margin="{ThemeResource ContentPageMargin}"
                HorizontalAlignment="Stretch"
                VerticalScrollBarVisibility="Auto">
        <StackPanel Margin="10"
                    wuc:PanelAttach.ChildrenTransitions="Default"
                    wuc:SettingsNavigationAttach.JsonNavigationViewService="{x:Bind local:App.Current.GetJsonNavigationViewService}"
                    Spacing="5">
            <!-- <wuc:SettingsCard Description="Your Description"
                              Header="Your Header"
                              HeaderIcon="{wuc:BitmapIcon Source=Assets/Fluent/icon.png}"
                              IsClickEnabled="True"
                              wuc:NavigationHelperEx.NavigateToSetting="views:MySettingPage" /> -->

        </StackPanel>
    </ScrollView>
</Page>

